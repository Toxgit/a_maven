//USEUNIT InputNodeHelper

function unlink()
{
  //initialization
  var inputNode = new InputNode.InputNode();
  
  //precondition
  Log.Message("Разлинковка переменных");

  //test bodys 
  clickListRowByName(inputNode.listVar, "IPN");
  inputNode.unlink();
  checkValueOfField(inputNode.listVar, "IPN", "Linked To", "");
  
  clickListRowByName(inputNode.listStrr, "IPN_");
  inputNode.link();
  checkValueOfField(inputNode.listVar, "IPN", "Linked To", "IPN_");
  //postcondition
}