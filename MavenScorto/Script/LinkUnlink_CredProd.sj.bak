//USEUNIT NodeHelper

function LinkUnlink_CredProd()
{
  //initialization
 
  //precondition
  Log.Message("Разлинковка переменных, линковка переменных");

  //test bodys 
  clickListRowByName(inputNode.listVar, "minSum");
  radiobuttonSetChecked(inputNode.alias.OutputVariables, 1);
  inputNode.unlink();
  checkValueOfField(inputNode.listVar, "minSum", "Linked To", "");
  clickListRowByName(inputNode.listStr, "minSum");
  inputNode.link();
  checkValueOfField(inputNode.listVar, "minSum", "Linked To", "minSum");
   
  Log.Message("****");
  Log.Message("Авто линковка");
  radiobuttonSetChecked(inputNode.alias.OutputVariables, 1);
  clickListRowByName(inputNode.listVar, "maxSum");
  inputNode.unlink();
  checkValueOfField(inputNode.listVar, "maxSum", "Linked To", "");
  inputNode.autoLink();
  checkValueOfField(inputNode.listVar, "maxSum", "Linked To", "maxSum");
  //postcondition
}