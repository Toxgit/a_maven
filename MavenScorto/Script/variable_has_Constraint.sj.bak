//USEUNIT Settingsы
//USEUNIT Toolbar
//USEUNIT VariablesHelper
//USEUNIT OpenStrHelper

function Constraint()
{
  //test variable properties
  //initialization
  var testStrategyPath =  strLocation + "Constraint.strat";//путь к стратегии для теста
  var toolbar = new Toolbar.Toolbar();
  var variables = new Variables.Variables();
  
  //precondition
  openStrByPath(testStrategyPath);
  toolbar.variables();
  
  //test bodys (проверка того что переменные содержат constraint)
   for(var i=0; i<ODT.Data.Variables.VariableCount; i++)
  {
    var item = ODT.Data.Variables.Variables(i).Value;
    if (item.options == "constraint")
    {
      clickRowByName(item.name);
      chooseProperty("Constraint name");
      setConstraintName(item.constraintName);
      clickRowByName(item.name);
      checkPropertiForSelectedVariable("Constraint name",item.constraintName);
      checkPropertiForSelectedVariable("Constraint values",item.constraintValue);
    }
  }
  //postcondition
  variables.ok();
  closeOneStrWhithWarning();
  
}
