function Check_InitialValue()
{
  //test variable properties
  //initialization
  var testStrategyPath =  strLocation + "Check_InitialValue.strat";//путь к стратегии для теста
  var toolbar = new Toolbar.Toolbar();
  var variables = new Variables.Variables();
  
  //precondition
  openStrByPath(testStrategyPath);
  toolbar.variables();
  
  //test bodys
   for(var i=0; i<ODT.Data.Variables.VariableCount; i++)
  {
    var item = ODT.Data.Variables.Variables(i).Value;
    if (item.options == "checking initial value")
    {
      if (item.expectedResult == "Fail")
      {
        clickRowByName(item.name);
        chooseProperty("Initial value");
        setDropDownList(item.initialValue);
        clickRowByName(item.name);
        checkPropertiForSelectedVariable("Initial value","NULL");
      }else
      {
        clickRowByName(item.name);
        chooseProperty("Initial value");
        setDropDownList(item.initialValue);
        clickRowByName(item.name);
        checkPropertiForSelectedVariable("Initial value",item.initialValue);
      }
    }
  }
  //postcondition
  variables.cancel();
  closeOneStrWhithWarning();
  
}