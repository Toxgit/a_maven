//добавление constraint переменной
function to_ManageConstraints()
{
  //initialization
  var testStrategyPath =  strLocation + "Constraint.strat";//путь к стратегии для теста
  var toolbar = new Toolbar.Toolbar();
  var variables = new Variables.Variables();
  var selectConstraint = new SelectConstraint.SelectConstraint();
  var constraint = new Constraint.Constraint();
  
  //precondition
  /*openStrByPath(testStrategyPath);
  toolbar.variables();  */
  
  //test body
  Log.Message("Тест: Opens Strategy Constraint dialog");
  
  clickRowByName("String2");
  chooseProperty("Constraint name");
  variables.toSelectConstraint();
  selectConstraint.toManageConstraints();
  
  if (constraint.alias.Exists)
  {
   
    Log.Checkpoint("Strategy Constraint dialog has opened")
    constraint.cancel();
  }
  
  
   
  //postcondition
  selectConstraint.cancel();
  variables.ok();
  toolbar.closeDoNotSave();
  
}